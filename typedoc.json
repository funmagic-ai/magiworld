{
  "$schema": "https://typedoc.org/schema.json",
  "name": "MagiWorld API Documentation",
  "entryPoints": [
    "apps/admin/lib",
    "apps/admin/components",
    "apps/admin/app",
    "apps/web/lib",
    "apps/web/components",
    "apps/web/app"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeNotDocumented": false,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Actions", "Components", "Utilities", "*"],
  "sort": ["source-order"],
  "tsconfig": "apps/admin/tsconfig.json"
}
